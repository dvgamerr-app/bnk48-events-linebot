{
  "name": "bnk48-events-linebot",
  "version": "1.0.0",
  "description": "bnk48 website and linebot api.",
  "author": "Mr.Kananek Thongkam",
  "private": true,
  "scripts": {
    "dev": "env-cmd .env nuxt",
    "serv": "env-cmd .env backpack dev",
    "build": "npm run eslint && nuxt build && backpack build",
    "start": "node dist/main.js",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "deploy": "npm run precommit && npm run build && npm run docker",
    "precommit": "git checkout . && git pull origin master && npm i --only=prod",
    "docker": "docker restart web-bnk48 && docker logs --follow web-bnk48"
  },
  "dependencies": {
    "@line/bot-sdk": "^6.4.0",
    "@nuxtjs/axios": "^5.0.0",
    "@touno-io/db": "^1.4.3",
    "@touno-io/debuger": "^1.4.5",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "bootstrap-vue": "^2.0.0-beta",
    "env-cmd": "^8.0.2",
    "express": "^4.16.3",
    "nuxt": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "backpack-core": "^0.7.0",
    "eslint": "^5.12.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-backpack": "^0.2.2",
    "eslint-plugin-vue": "^5.1.0",
    "nodemon": "^1.11.0",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0"
  }
}
